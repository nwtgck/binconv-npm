  "homepage": "https://github.com/nwtgck/binconv-npm#readme",
  "devDependencies": {
    "@types/jasmine": "^3.5.1",
    "@types/node": "^15.12.3",
    "@types/power-assert": "^1.5.3",
    "jasmine-core": "^3.5.0",
    "karma": "^5.0.9",
    "karma-chrome-launcher": "^3.1.0",
    "karma-cli": "^2.0.0",
    "karma-firefox-launcher": "^2.1.0",
    "karma-jasmine": "^4.0.1",
    "karma-typescript": "^5.0.0",
    "power-assert": "^1.6.1",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5"
  }
